        "# css 찾을때 까지 10초 대기\n",
        "def time_wait(num, code):\n",
        "  try:\n",
        "    wait = WebDriverWait(driver, num).until(\n",
        "      EC.presence_of_element_located((By.CSS_SELECTOR, code)))\n",
        "  except:\n",
        "    print(code, '태그를 찾지 못하였습니다.')\n",
        "    driver.quit()\n",
        "  return wait"
